{
  "name": "ED-WIOA-subtheme",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bixal/wioa-state-plan"
  },
  "scripts": {
    "prebuild:css": "node-sass --include-path scss sass/styles.scss assets/css/styles.css",
    "build:css": "postcss --use autoprefixer -b 'last 2 versions' < assets/css/styles.css | postcss --use cssnano --map -o assets/css/styles.min.css",
    "build": "npm run build:css",
    "build:watch": "watch 'npm run build' sass"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "cssnano": "^3.10.0",
    "gulp": "^4.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-iconfont": "^10.0.2",
    "gulp-iconfont-css": "^2.3.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.9",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.2",
    "postcss-cli": "^4.1.0",
    "uswds": "^1.4.2",
    "watch": "^1.0.2"
  }
}
